
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Nome</th>
                <th>Quantidade</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var produto in ViewBag.Produto)
            {
                <tr>@produto.Id</tr>
                <tr>@produto.Nome</tr>
                <tr>@produto.Quantidade</tr>
            }
            <tr></tr>
        </tbody>
    </table>
</body>
</html>
